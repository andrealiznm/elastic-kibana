[ req ]
default_bits       = 2048
distinguished_name = req_distinguished_name
req_extensions     = req_ext
prompt             = no

[ req_distinguished_name ]
C  = CO
O  = ElasticLab
OU = Nodes
CN = kibana

[ req_ext ]
subjectAltName = @alt_names

[ alt_names ]
DNS.1 = kibana
DNS.2 = localhost
IP.1  = 127.0.0.1
